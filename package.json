{
  "name": "react-component-library",
  "version": "1.0.0",
  "main": "build/main.js",
  "files": [
    "build"
  ],
  "types": "build/index.d.ts",
  "description": "A skeleton to create your own React component library using Webpack, TypeScript, Sass and Storybook",
  "scripts": {
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "start-storybook -p 6006",
    "storybook:export": "build-storybook",
    "generate": "node ./util/create-component",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HarveyD/react-component-library.git"
  },
  "keywords": [
    "React",
    "Component",
    "Library",
    "Rollup",
    "Typescript",
    "Sass",
    "Storybook"
  ],
  "author": "Harvey Delaney",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HarveyD/react-component-library/issues"
  },
  "homepage": "https://github.com/HarveyD/react-component-library#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@types/jest": "^24.0.24",
    "@types/react": "^16.9.12",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^8.0.1",
    "typescript": "^3.7.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
